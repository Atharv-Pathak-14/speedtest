{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "defaults, not dead",
        "loose": true,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-template-literals"
  ]
}
